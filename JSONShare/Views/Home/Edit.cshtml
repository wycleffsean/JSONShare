@model JSONShare.Models.JsonItem

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<pre>Created: @Model.Created</pre>
<pre>Updated: @Model.Updated</pre>
<pre>Viewed: @Model.Viewed</pre>

@Model.Created

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    @Html.EditorFor(m => m)  
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
